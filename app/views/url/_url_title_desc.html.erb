<h4 class='url_title'>
  <%= link_to title, url, :target => '_blank' %>
</h4>
<p class='url_desc'>
  <%= link_to content, url, :target => '_blank' %>
</p>
<%= hidden_field :experience, :url_title, {:value => title} %>
<%= hidden_field :experience, :url_content, {:value => "#{content}"} %>
<%= hidden_field :experience, :url, {:value => url} %>

